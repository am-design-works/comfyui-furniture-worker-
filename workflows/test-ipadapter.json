      "ipadapter": ["1", 0],
      "image": ["4", 0],
      "clip_vision": ["2", 0]
    },
    "class_type": "IPAdapterAdvanced",
    "_meta": { "title": "Apply IP-Adapter" }
  },
  "6": {
    "inputs": {
      "text": "product in modern office environment, professional photography, natural lighting",
      "clip": ["3", 1]
    },
    "class_type": "CLIPTextEncode"
  },
  "7": {
    "inputs": {
      "text": "blurry, distorted, low quality",
      "clip": ["3", 1]
    },
    "class_type": "CLIPTextEncode"
  },
  "8": {
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "class_type": "EmptyLatentImage"
  },
  "9": {
    "inputs": {
      "seed": 42,
      "steps": 25,
      "cfg": 7.0,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1,
      "model": ["5", 0],
      "positive": ["6", 0],
      "negative": ["7", 0],
      "latent_image": ["8", 0]
    },
    "class_type": "KSampler"
  },
  "10": {
    "inputs": {
      "samples": ["9", 0],
      "vae": ["3", 2]
    },
    "class_type": "VAEDecode"
  },
  "11": {
    "inputs": {
      "filename_prefix": "ipadapter_test",
      "images": ["10", 0]
    },
    "class_type": "SaveImage"
  }
