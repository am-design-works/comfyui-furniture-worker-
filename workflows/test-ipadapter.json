{
  "_meta": {
    "title": "Test: IP-Adapter Verification",
    "description": "Test workflow to verify IP-Adapter nodes are working"
  },
  "1": {
    "inputs": {
      "ipadapter_file": "ip-adapter-plus_sdxl_vit-h.safetensors"
    },
    "class_type": "IPAdapterModelLoader",
    "_meta": { "title": "Load IP-Adapter" }
  },
  "2": {
    "inputs": {
      "clip_name": "CLIP-ViT-H-14-laion2B-s32B-b79K.safetensors"
    },
    "class_type": "CLIPVisionLoader",
    "_meta": { "title": "Load CLIP Vision" }
  },
  "3": {
    "inputs": {
      "ckpt_name": "RealVisXL_V4.0.safetensors"
    },
    "class_type": "CheckpointLoaderSimple",
    "_meta": { "title": "Load Checkpoint" }
  },
  "4": {
    "inputs": {
      "image": "product.png"
    },
    "class_type": "LoadImage",
    "_meta": { "title": "Load Product Image" }
  },
  "5": {
    "inputs": {
      "weight": 0.8,
      "weight_type": "linear",
      "combine_embeds": "concat",
